{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "comps/*": [
                "src/components/*"
            ],
            "store/*": [
                "src/store/*"
            ]
        }
    },
    "include": [
        "./src/**/*"
    ]
}